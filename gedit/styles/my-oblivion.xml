<style-scheme name="My-Oblivion" id="my-oblivion" version="1.0">
  <author>Paolo Borelli</author>
  <description>Tmavé barevné schéma dle barevné palety Tango</description>

  <style name="text"	foreground="#d3d7cf" background="#2b2b2b" bold="true" />
  <style name="selection"	background="#9b2b2b" />
  <style name="cursor"	foreground="#d3d7cf" />
  <style name="current-line"	background="#181818" bold="true" />
  <style name="line-numbers"	foreground="#babdb6" background="#232323" />
  <style name="draw-spaces"	foreground="#888a85" />
  <style name="bracket-match"	foreground="#c17d11" bold="true" />
  <style name="bracket-mismatch"	foreground="#eeeeec" background="#cc0000" bold="true" />
  <style name="right-margin"	foreground="#eeeeec" background="#babdb6" />
  <style name="search-match"	foreground="#eeeeec" background="#4e9a06" />
  <style name="def:comment"	foreground="#83bf7f" bold="true" />
  <style name="def:doc-comment-element"	italic="true" />
  <style name="def:constant"	foreground="#ffd046" bold="true" />
  <style name="def:string"	foreground="#b69ee2" />
  <style name="def:special-char"	foreground="#8ae234" />
  <style name="def:special-constant"	foreground="#d49197" bold="true" />
  <style name="def:floating-point"	foreground="#edd400" bold="true" />
  <style name="def:identifier"	foreground="#e9b96e" bold="true" />
  <style name="def:statement"	foreground="#ffffff" bold="true" />
  <style name="def:preprocessor"	foreground="#d5a2cf" bold="true" />
  <style name="def:error"	foreground="#eeeeec" background="#cc0000" bold="true" />
  <style name="def:warning"	foreground="#eeeeec" background="#ad7fa8" />
  <style name="def:note"	foreground="#000000" background="#83bf80" bold="true" />
  <style name="def:underlined"	italic="true" underline="true" />
  <style name="diff:added-line"	foreground="#edd400" />
  <style name="diff:removed-line"	foreground="#729fcf" />
  <style name="diff:changed-line"	foreground="#ad7fa8" />
  <style name="diff:diff-file"	foreground="#8ae234" bold="true" />
  <style name="diff:location"	foreground="#8ae234" />
  <style name="diff:special-case"	foreground="#ffffff" bold="true" />
  <style name="latex:display-math"	foreground="#5c3566" />
  <style name="latex:command"	foreground="#8ae234" bold="true" />
  <style name="latex:include"	foreground="#ad7fa8" />
  <style name="python:module-handler"	foreground="#d589c0" bold="true" />
  <style name="boolean"	foreground="#edd400" />
  <style name="python:boolean"	foreground="#edd400" bold="true" />
  <style name="python:floating-point"	foreground="#edd400" bold="true" />
  <style name="def:type"	foreground="#fcaf3e" bold="true" />
  <style name="number"	foreground="#edd400" bold="true" />
  <style name="fortran:boz-literal"	underline="true" />
</style-scheme>
