[qpdfview Shortcut Group]
Name=Prohlížet PDF dokumenty
Exec=qpdfview --unique --instance "main"
TargetEnvironment=Unity

[medeley Shortcut Group]
Name=Správa PDF dokumentů
Exec=/home/jaandrle/.local/share/mendeleydesktop
TargetEnvironment=Unity

[acrobat Shortcut Group]
Name=Editovat PDF dokumenty
Exec=wine /home/jaandrle/.wine/drive_c/Program\ Files/Adobe/Acrobat\ 7.0/Acrobat/Acrobat.exe
TargetEnvironment=Unity

[k2pdfopt Shortcut Group]
Name=Optimalizovat PDF dokumenty
Exec=wine /home/jaandrle/UlozeneSoubory/k2pdfopt.exe
TargetEnvironment=Unity

[Verdict Shortcut Group]
Name=Přeložit pomocí Verdict
Exec=wine /home/jaandrle/.wine/drive_c/Program\ Files/Verdict\ Free/Verdict.exe
TargetEnvironment=Unity

[Peki Shortcut Group]
Name=Přeložit pomocí PEKI
Exec=java -jar '/home/jaandrle/Uložené soubory/PEKI dictionary/data/peki.jar'
TargetEnvironment=Unity

[medeleysafe Shortcut Group]
Name=Správa PDF dokumentů (safe)
Exec=bash -c 'QT_X11_NO_MITSHM=1 /home/jaandrle/.local/share/mendeleydesktop'
TargetEnvironment=Unity
