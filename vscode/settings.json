{
    "files.autoSave": "afterDelay",
    "editor.fontSize": 9.5,
    "editor.fontFamily": "'iA Writer Duo S','Ubuntu Mono',monospace",
    "editor.renderWhitespace": "boundary",
    "explorer.compressSingleChildFolder": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.*~": true
    },
    "editor.fontWeight": "bold",
    "editor.tabCompletion": "on",
    "window.menuBarVisibility": "toggle",
    "workbench.editor.showTabs": false,
    "editor.minimap.maxColumn": 60,
    "sync.gist": "4eebd99cf93d5c93abd5b7b70ee996f4",
    "sync.quietSync": false,
    "sync.askGistName": false,
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "diffEditor.ignoreTrimWhitespace": true,
    "php.validate.executablePath": "C:\\wamp64\\bin\\php\\php5.6.31\\php.exe",
    "editor.semanticTokenColorCustomizations": {
        "[Default Dark+]": {
            "enabled": true,
            "rules": {
                "*.readonly": "#c4c5b8",
                "property": "#a4d7e2",
                "function": "#eec691"
            }
        }
     },
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            "comments": "#9ed49a",
            "types": "#fff",
            "textMateRules": [
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "support.variable",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "support.function",
                    "settings": {
                        "foreground": "#eeb76f"
                    }
                },
                {
                    "scope": "string.quoted",
                    "settings": {
                        "foreground": "#91aaee"
                    }
                },
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "variable.other",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#eec691"
                    }
                },
                {
                    "scope": "meta",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "keyword.operator.new",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "storage.type",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "storage.modifier",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "punctuation.terminator",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        }
    },
    "highlight.regexes": {
        "(_this)":{
            "filterLanguageRegex": "JavaScript",
            "decorations":[
                {"color": "white"}
            ]
        },
        "(gulp_place)":{
            "decorations":[
                {"color": "#fbca04"}
            ]
        }
    },
    "window.zoomLevel": 0,
    "breadcrumbs.enabled": true,
    "window.newWindowDimensions": "inherit",
    "terminal.integrated.rendererType": "dom",
    "git.enabled": false,
    "bracket-pair-colorizer-2.colors": [
        "#C7A349",
        "#e6aad0",
        "#FFFFFF"
    ],
    "sync.forceUpload": true,
    "task.autoDetect": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "metaGo.decoration.backgroundColor": "#2b2b2b,#2b2b2b",
    "metaGo.decoration.color": "yellow",
    "metaGo.decoration.backgroundOpacity": "1",
    "metaGo.decoration.borderColor": "yellow",
    "metaGo.jumper.screenLineRange": 80,
    "metaGo.jumper.findAllMode": "off",
    "metaGo.decoration.x": 3,
    "metaGo.decoration.width": 6,
    "metaGo.jumper.wordSeparatorPattern": "[\\{\\}\\[\\]\\(\\) ,._!?;\"'`<>\\/+\\-*$]",
    "vim.visualstar": true,
    "vim.easymotion": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["i", "i"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [ ";" ],
            "after": [ ":" ]
        },
        {
            "before": [ "U" ],
            "after": [ "<c-r>" ]
        },
        {
           "before": [ "ž" ],
            "after": [ "^" ]
        },
        {
           "before": [ "č" ],
            "after": [ "$" ]
        },
        {
            "before": [ "K" ],
            "after": [ "a", "<cr>", "<esc>" ]
        },
        {
            "before": [ "<leader>", "c", "w" ],
            "after": [ "*", "`", "`", "c", "g", "n" ]
        },
        {
            "before": [ "<leader>", "c", "b" ],
            "after": [ "#", "`", "`", "c", "g", "N" ]
        },
        {
            "before": [ "<leader>", "p", "a" ],
            "after": [ "a", "<space>", "<esc>", "p" ]
        },
        {
            "before": [ "<leader>", "p", "A" ],
            "after": [ "A", "<space>", "<esc>", "p" ]
        },
        {
            "before": [ "<leader>", "p", "i" ],
            "after": [ "i", "<space>", "<esc>", "P" ]
        },
        {
            "before": [ "<leader>", "p", "I" ],
            "after": [ "I", "<space>", "<esc>", "P" ]
        },
        {
            "before": [ "<leader>", "O" ],
            "after": [ "O", " ", "<BS>", "<Esc>" ]
        },
        {
            "before": [ "<leader>", "o" ],
            "after": [ "o", " ", "<BS>", "<Esc>" ]
        },
        {
            "before": [ "<leader>", ";", "o" ],
            "after": [ "A", ";", "<Esc>", "o", " ", "<BS>" ]
        },
        {
            "before": [ "<leader>", ",", "o" ],
            "after": [ "A", ",", "<Esc>", "o", " ", "<BS>" ]
        },
        {
            "before": [ "ú" ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": ["g", "s"],
            "commands": [
                "workbench.action.showAllSymbols"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p",
            ],
            "after": [
                "p",
                "g",
                "v",
                "y"
            ]
        }
    ],
    "vim.history": 500,
    "vim.textwidth": 120,
    "vim.vimrc.enable": false,
    "vim.disableExtension": true,
    "search.enableSearchEditorPreview": true,
    "vim.easymotionMarkerFontFamily": "iA Writer Duo S",
    "vim.easymotionMarkerFontSize": "9,5",
    "rest-client.fontSize": 9.5,
    "rest-client.fontWeight": "bold",
    "terminal.integrated.fontSize": 9.5,
    "terminal.integrated.lineHeight": 0.95,
    "terminal.integrated.letterSpacing": -0.1,
    "editor.wordWrapColumn": 120,
    "settings.cycle": [
        {
            "id": "wordWrap",
            "overrideWorkspaceSettings": false,
            "values": [
                {
                    "editor.wordWrap": "wordWrapColumn",
                },
                {
                    "editor.wordWrap": "off",
                }
            ]
        }
    ],
    "editor.wordWrap": "off",
    "editor.snippetSuggestions": "top",
    "typescript.workspaceSymbols.scope": "currentProject",
}