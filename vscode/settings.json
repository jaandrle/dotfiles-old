                },
                {
                    "scope": "storage.modifier",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "keyword.control",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "punctuation.terminator",
                    "settings": {
                        "foreground": "#fff"
                    }
                }
            ]
        }
    },
    "highlight.regexes": {
        "(_this(?=\\.|[ ]*\\)))":{
            "filterLanguageRegex": "JavaScript",
            "decorations":[
                {"color": "white"}
            ]
        },
        "((?<=\\/\\* global )([^, \\*]+,? ?)+(?= \\*\\/))":{
            "filterLanguageRegex": "JavaScript",
            "decorations":[
                {"color": "#fbca04"}
            ]
        },
        "((?<=\\/\\* )global (?=([^, \\*]+,? ?)+ \\*\\/))":{
            "filterLanguageRegex": "JavaScript",
            "decorations":[
                {"color": "white"}
            ]
        },
        "(gulp_place(?=\\([ ]*(\"[^\\)\"]+\"|'[^\\)']+')[ ]*,[ ]*[\"'](file|file_once|file_if_exists|eval|eval_out|variable|files|glob|files_once|glob_once|clean|combine)[\"'][ ]*\\)))":{
