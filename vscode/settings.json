                    }
                },
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "variable.other",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#eec691"
                    }
                },
                {
                    "scope": "meta",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "keyword.operator.new",
                    "settings": {
                        "foreground": "#d0bcf0"
                    }
                },
                {
                    "scope": "storage.type",
                    "settings": {
                        "foreground": "#d0bcf0"
