            "textMateRules": [
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#fff"
                    }
                },
                {
                    "scope": "support.variable",
                    "settings": {
                        "foreground": "#e4e5d8"
                    }
                },
                {
                    "scope": "support.function",
                    "settings": {
                        "foreground": "#eeb76f"
                    }
                },
                {
                    "scope": "string.quoted",
                    "settings": {
