        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+i",
        "command": "-metaGo.selectLineDown",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+b",
        "command": "-metaGo.currentLineToBottom",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+f3 oem_6",
        "command": "editor.action.jumpToBracket",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+oem_5",
        "command": "-editor.action.jumpToBracket",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+f3 f4",
        "command": "toggleVim"
    },
    {
        "key": "alt+f3 f6",
        "command": "notifications.showList"
    }
]