    {
        "key": "alt+t",
        "command": "-metaGo.currentLineToTop",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+backspace",
        "command": "-metaGo.delete.softDelete",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+;",
        "command": "-metaGo.goto",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+f3 shift+f4",
        "command": "metaGo.selection",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+;",
        "command": "-metaGo.selection",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+f3 f4",
        "command": "metaGo.gotoBefore",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+;",
        "command": "-metaGo.gotoBefore",
        "when": "editorTextFocus"
